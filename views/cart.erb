<div id="cart"> 
	<table class="table table-striped">
		<thead>
		<tr>
		  <th>#</th>
		  <th>Name</th>
		  <th>Price</th>
		  <th>Qty</th>
		</tr>
		</thead>
		<tbody>
		
		<% @orders.each do |item| %>	
		<tr>
		  <th> <%= item[0].id %></th>
		  <td> <%= item[0].title %></td>
		  <td><%= item[0].price %></td>
		  <td><%= item[1] %></td>
		</tr>
		<% end%>
